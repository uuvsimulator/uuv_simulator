  <license>Apache-2.0</license>

  <buildtool_depend>catkin</buildtool_depend>

  <run_depend>uuv_trajectory_control</run_depend>
  <run_depend>uuv_control_utils</run_depend>
  <run_depend>uuv_descriptions</run_depend>
